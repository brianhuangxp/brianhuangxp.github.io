    <title>markdown example</title>
    <link href="https://brianhuangxp.github.io/2018/08/10/markdown.example/"/>
    <id>https://brianhuangxp.github.io/2018/08/10/markdown.example/</id>
    <published>2018-08-09T16:00:00.000Z</published>
